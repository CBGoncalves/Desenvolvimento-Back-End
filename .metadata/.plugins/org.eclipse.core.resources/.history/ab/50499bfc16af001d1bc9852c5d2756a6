package exerciciosFaccat;

import java.util.Scanner;

public class Exercicio05 {
	
	public static void main(String args[]) {
		
		int num;
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Digite um n√∫mero: ");
		num = sc.nextInt();
		
		num = num - 1;
		
		System.out.println(num);
		sc.close();
		
	}

}
